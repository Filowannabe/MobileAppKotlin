<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crudfelipe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CRUDFELIPE">
        <activity
            android:name=".EditStudentActivity"
            android:exported="true"
            android:label="@string/title_activity_edit_student"
            android:theme="@style/Theme.CRUDFELIPE.NoActionBar" />
        <activity
            android:name=".DetailStudentActivity"
            android:exported="true"
            android:label="@string/title_activity_detail_student"
            android:theme="@style/Theme.CRUDFELIPE.NoActionBar" />
        <activity
            android:name=".AddStudentActivity"
            android:exported="true"
            android:label="@string/title_activity_add_student"
            android:theme="@style/Theme.CRUDFELIPE.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>